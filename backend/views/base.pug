            script.
                $(function(){
                search_width = $(".searchform").width()
                $('#search').autocomplete({
                    appendTo: ".searchform",
                    autoFocus: true,
                source: function(req,res){
                $.ajax({
                    url:"/searchbar?key=Home",
                    dataType:"jsonp",
                    type:"GET",
                    data:req,
                    success: function(data){
                    res(data)
                    },
                    error: function(err){
                    console.log(err.status);
                    }
                });
                },
                    classes: {
                        "ui-autocomplete": "highlight"
                    },
                    autofocus:true,
                minLength:1,
                select: function(event,ui){
                if(ui.item){
                    $('#search').val(ui.item.label);
                }
                },
                open: function(){
                    $('.ui-autocomplete').css('left', '0px'); // HERE
                    $('.ui-autocomplete').css('position', 'absolute'); // HERE
                    $('.ui-autocomplete').css('width', `${search_width}px`); // HERE

                    $('body').toggleClass('changed');
                },
                close: function(){
                    $('body').removeClass('changed');
                }
                });
                });
